def cubes_program():
    lst = [1, 2, 'a', 4.5, 6, 3, 8, 'hello', 10]
    cubes_for = []
    for item in lst:
        if isinstance(item, int) and item % 2 == 0:
            cubes_for.append(item**3)
    cubes_lc = [x**3 for x in lst if isinstance(x,int) and x%2==0]
    print("Cubes (loop):", cubes_for)
    print("Cubes (list comprehension):", cubes_lc)
